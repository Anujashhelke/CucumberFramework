pipeline{
    agent{label 'windows'}
    tools{
        maven 'M2'
    }
    stages{
        stage('checkout'){
            steps{
               git' https://github.com/Anujashhelke/Demo.git'
            }
        }
        stage('build'){
            steps{
                sh 'mvn clean compile'
            }
        }
        stage('Test'){
            steps{
                sh 'mvn test'
                junit '**/target/surefire-report/Test-*.xml'
            }

        }
        stage('package'){
            steps{
                sh 'mvn package'
            }
        }
    }
}